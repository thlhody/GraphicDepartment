<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cottontex.graphdep.controllers.LauncherController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label style="-fx-text-fill: Black;" text="Login" />
    <Label fx:id="errorMessageLabel" style="-fx-text-fill: Red;" text="" />
    <TextField fx:id="usernameField" maxHeight="25.0" maxWidth="100.0" minHeight="25.0" minWidth="100.0" prefHeight="25.0" prefWidth="100.0" promptText="Username" />
    <PasswordField fx:id="passwordField" maxHeight="25.0" maxWidth="100.0" minHeight="25.0" minWidth="100.0" prefHeight="25.0" prefWidth="100.0" promptText="Password" />
    <Button fx:id="loginButton" maxHeight="25.0" maxWidth="80.0" minHeight="25.0" minWidth="80.0" onAction="#onLoginButtonClick" prefHeight="25.0" prefWidth="80.0" text="Login" />
</VBox>
